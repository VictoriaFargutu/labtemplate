#VRML V2.0 utf8
# Overlapping shapes
#calculator
 

Shape {
        geometry Box {
                size 5 7.5 1
        }

        appearance Appearance {
		material Material { diffuseColor 0 0.5 0.5}
	}

        
}

Transform {
   # The display
   translation 0 2.64 0.5
   children [
      Shape {
         geometry Box { size 4.4 1.7 0.3 }
         appearance Appearance {
             material Material {
                    diffuseColor 0.65, 0.86, 0.99 # white in RGB format
                    
                }
         }
      }
       Transform{ translation -0.1 -0.2 0.2
      children[

          Shape{
          
          geometry Text{ string "132 x 72"
          
           fontStyle FontStyle{ size 0.5}
          }

          appearance Appearance {
                 material Material { diffuseColor 1 0 0 }
   }

           }
                
          ] 
     
   
          
      }
     ]
}

Transform {
   # The button"%"
